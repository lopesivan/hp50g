CST {
  { \|^ \<< UPDIR LoadCST \>> }

  XY\<-\->P

  D\<-\->R

  {
  R\<-\->C
  \<<
    IF -103. FS?
    THEN -103. usetf
    ELSE -103. setf
    END
  \>>
  }

  {
  " = "
  \<<
    -105 usetf
    -3 usetf
    -17 setf
  \>>
  }

  { " ~ "
  \<<
    -105 setf
    -3 setf
    -17 usetf
  \>>
  }

  {
  C\->R
  \<<
    IF -128 FS?
    THEN -128 usetf
    ELSE -128 setf
    END
  \>>
  }

  { expand EXPAND }
  { factor FACTOR }
  {"Q" q}
}
